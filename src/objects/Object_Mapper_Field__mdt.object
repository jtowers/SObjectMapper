        <defaultValue>true</defaultValue>
        <externalId>false</externalId>
        <label>Mapping Enabled</label>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Source_Field_API_Name__c</fullName>
        <externalId>false</externalId>
